{
	//项目名字或项目绝对路径
	"project": "sclm-app",
	//打包平台 默认值android  值有"android","ios" 如果要打多个逗号隔开打包平台
	"platform":"ios",
	//是否使用自定义基座 默认值false  true自定义基座 false自定义证书
	"iscustom": false,
	//打包方式是否为安心打包默认值false,true安心打包,false传统打包
	"safemode": false,
	//ios打包参数
	"ios": {
		//ios appid
		"bundle":"com.sclm.test",
		//ios打包支持的设备类型 默认值iPhone 值有"iPhone","iPad" 如果要打多个逗号隔开打包平台
		"supporteddevice":"iPhone,iPad",
		//iOS使用自定义证书打包的profile文件路径
		"profile":"E:/AAA项目/sclm/证书/ios/ios.developer.mobileprovision",
		//iOS使用自定义证书打包的p12文件路径
		"certfile":"E:/AAA项目/sclm/证书/ios/ios.developer.p12",
		//iOS使用自定义证书打包的证书密码
		"certpassword":"sclm123dev"
	},
	//是否混淆 true混淆 false关闭
	"isconfusion":false,
	//开屏广告 true打开 false关闭
	"splashads":false,
	//悬浮红包广告true打开 false关闭
	"rpads":false,
	//push广告 true打开 false关闭
	"pushads":false,
	//加入换量联盟 true加入 false不加入
	"exchange":false
}
